describe('VideoPlayer time formatting', function () {
  var formatTime = function formatTime(ms) {
    var totalSeconds = Math.floor(ms / 1000);
    var minutes = Math.floor(totalSeconds / 60);
    var seconds = totalSeconds % 60;
    return `${minutes}:${seconds.toString().padStart(2, '0')}`;
  };
  it('should format 0 milliseconds correctly', function () {
    expect(formatTime(0)).toBe('0:00');
  });
  it('should format seconds correctly', function () {
    expect(formatTime(5000)).toBe('0:05');
    expect(formatTime(30000)).toBe('0:30');
    expect(formatTime(59000)).toBe('0:59');
  });
  it('should format minutes correctly', function () {
    expect(formatTime(60000)).toBe('1:00');
    expect(formatTime(90000)).toBe('1:30');
    expect(formatTime(120000)).toBe('2:00');
  });
  it('should format longer durations correctly', function () {
    expect(formatTime(600000)).toBe('10:00');
    expect(formatTime(3600000)).toBe('60:00');
  });
});
